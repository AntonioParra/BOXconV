<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:control="com.cubiclan.boxconv.control">
	<Dialog>
        <customHeader>
            <Toolbar>
                <Title text="{file>/object/label}"></Title>
                <ToolbarSpacer />
                <Button icon="sap-icon://decline" press="closeDownloadPopup"></Button>
            </Toolbar>
        </customHeader>
        <HBox width="100%">
            <Button text="{i18n>download}" press="onDownloadPopup"/>
        </HBox>
        
    </Dialog>
</core:FragmentDefinition>